/*
* LIVERY SELECTION
*/

random_switch(FEAT_TRAINS, PARENT, switch_emu_Future_DD_randomlivery) {
	1: switch_emu_Future_DD_Sprinter_position;
	1: switch_emu_Future_DD_IC_position;
	1: switch_emu_Future_DD_Fyra_position;
	1: switch_emu_Future_DD_blackyellow_position;
	1: switch_emu_Future_DD_Veolia_position;
	1: switch_emu_Future_DD_Syntus_position;
	1: switch_emu_Future_DD_Arriva_position;
	1: switch_emu_Future_DD_Thalys_position;
	1: switch_emu_Future_DD_ICE_position;
}

switch(FEAT_TRAINS, SELF, switch_emu_Future_DD_livery, cargo_subtype) {
	1: switch_emu_Future_DD_Sprinter_position;
	2: switch_emu_Future_DD_IC_position;
	3: switch_emu_Future_DD_Fyra_position;
	4: switch_emu_Future_DD_blackyellow_position;
	5: switch_emu_Future_DD_Veolia_position;
	6: switch_emu_Future_DD_Syntus_position;
	7: switch_emu_Future_DD_Arriva_position;
	8: switch_emu_Future_DD_Thalys_position;
	9: switch_emu_Future_DD_ICE_position;
    switch_emu_Future_DD_randomlivery;
}

// CARGO SUBTYPE CALLBACK
switch(FEAT_TRAINS, SELF, switch_emu_Future_DD_subtype_text, cargo_subtype) {
    0: return string(str_cargo_subtype_random);
	1: return string(str_cargo_subtype_ns_sprinter);
    2: return string(str_cargo_subtype_ns_ic);
	3: return string(str_cargo_subtype_fyra_white);
    4: return string(str_cargo_subtype_black);
	5: return string(str_cargo_subtype_Veolia);
    6: return string(str_cargo_subtype_Syntus);
	7: return string(str_cargo_subtype_Arriva);
	8: return string(str_cargo_subtype_TGV);
    9: return string(str_cargo_subtype_ICE);	
    return CB_RESULT_NO_TEXT;
}

/*
* ARTICULATED VEHICLE CALLBACK
*/

switch(FEAT_TRAINS, SELF, switch_emu_Future_DD_articulated, extra_callback_info1) {
    1..9: return item_emu_Future_DD;
    return CB_RESULT_NO_MORE_ARTICULATED_PARTS;
}

/*
* VISUAL EFFECT AND POWERED CALLBACK
*/

switch(FEAT_TRAINS, SELF, switch_emu_Future_DD_visual_effect_and_powered, (position_in_vehid_chain % 10)) {
    0: return visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC, 0, DISABLE_WAGON_POWER);
    return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
}

